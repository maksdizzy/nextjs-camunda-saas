      <bpmn:script>const chain_id = execution.getVariable("form_chain_id");

const tx_hash = execution.getVariable("transfer_txhash");

if (chain_id == 261) {
  execution.setVariable("link", "https://sepolia.gurunetwork.ai/tx/" + tx_hash)
}
else {
  execution.setVariable("link", "https://scan.gurunetwork.ai/tx/" + tx_hash)
}</bpmn:script>
