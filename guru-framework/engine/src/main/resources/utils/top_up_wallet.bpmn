    <bpmn:scriptTask id="script_prepare_transaction" name="Prepare transaction" scriptFormat="javascript">
      <bpmn:extensionElements />
      <bpmn:incoming>Flow_0dnttcv</bpmn:incoming>
      <bpmn:outgoing>Flow_0jp4mol</bpmn:outgoing>
      <bpmn:script>// Get required variables
var tokenAddress = execution.getVariable("token_address");
var walletAddress = execution.getVariable("wallet_address");
var amount = execution.getVariable("form_amount");
var tokenDecimals = execution.getVariable("token_decimals");
var chainId = execution.getVariable("chain_id");

// Define constants
var NATIVE_TOKEN_ADDRESS = "0xeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeeee";
var isNativeTransfer = (tokenAddress === NATIVE_TOKEN_ADDRESS);

// Prepare variables
var contractAddress = isNativeTransfer ? walletAddress : tokenAddress;
var abi = isNativeTransfer ? "[]" : '[{"inputs":[{"name":"to","type":"address"},{"name":"value","type":"uint256"}],"name":"transfer","outputs":[{"name":"","type":"bool"}],"stateMutability":"nonpayable","type":"function"}]';
var functionName = isNativeTransfer ? "" : "transfer";
var functionInputs = isNativeTransfer ? "[]" : '[\"' + walletAddress + '\", \"' + (amount * Math.pow(10, tokenDecimals)).toString() + '\"]';
var txValue = isNativeTransfer ? amount * Math.pow(10, tokenDecimals) : 0;

// Store variables in execution context
execution.setVariable("web3_contractAddress", contractAddress);
execution.setVariable("web3_abi", abi);
execution.setVariable("web3_functionName", functionName);
execution.setVariable("web3_functionInputs", functionInputs);
execution.setVariable("web3_value", txValue.toString());
execution.setVariable("web3_chainId", chainId.toString());
</bpmn:script>
    </bpmn:scriptTask>
