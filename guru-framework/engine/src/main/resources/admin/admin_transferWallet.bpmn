const toPadded = recipientAddress.toLowerCase().replace("0x", "").padStart(64, "0");
const tokenIdHex = Number(tokenId).toString(16).padStart(64, "0");

// Final calldata
const data = methodId + fromPadded + toPadded + tokenIdHex;

execution.setVariable("to", tokenAddress);
execution.setVariable("value", "0");
execution.setVariable("data", data);
</bpmn:script>
